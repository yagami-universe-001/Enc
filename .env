# 144p - ultra low
FFMPEG=ffmpeg -i {} -vf scale=-2:144 -c:v libx264 -preset ultrafast -crf 35 -c:a aac -b:a 32k {}_144p.mp4

# 240p - low
FFMPEG2=ffmpeg -i {} -vf scale=-2:240 -c:v libx264 -preset ultrafast -crf 32 -c:a aac -b:a 48k {}_240p.mp4

# 360p - medium-low
FFMPEG3=ffmpeg -i {} -vf scale=-2:360 -c:v libx264 -preset veryfast -crf 28 -c:a aac -b:a 64k {}_360p.mp4

# 480p - standard definition
FFMPEG4=ffmpeg -i {} -vf scale=-2:480 -c:v libx264 -preset faster -crf 25 -c:a aac -b:a 96k {}_480p.mp4

# 720p - HD
FFMPEG5=ffmpeg -i {} -vf scale=-2:720 -c:v libx264 -preset medium -crf 23 -c:a aac -b:a 128k {}_720p.mp4

# 1080p - Full HD
FFMPEG6=ffmpeg -i {} -vf scale=-2:1080 -c:v libx264 -preset slow -crf 20 -c:a aac -b:a 160k {}_1080p.mp4

# 2160p - 4K Ultra HD
FFMPEG7=ffmpeg -i {} -vf scale=-2:2160 -c:v libx265 -preset slow -crf 18 -c:a aac -b:a 192k {}_2160p.mp4

